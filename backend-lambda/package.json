{
  "name": "s3-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "ts-auto-guard --debug && tsc",
    "watch": "tsc -w",
    "prestart": "npm run compile",
    "start": "node dist/src/index.js",
    "lint": "eslint --ext .ts ./",
    "build": "npm run clean && npm run compile && npm run build.prepare && npm run build.zip",
    "build.prepare": "del-cli dist/test && cp package.json package-lock.json dist && cd dist && npm ci --only=production && cd ..",
    "build.zip": "cd dist && bestzip ../build.zip . && cd ..",
    "clean": "del-cli dist build.zip"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.370.0",
    "@aws-sdk/client-ssm": "^3.370.0",
    "@aws-sdk/credential-providers": "^3.370.0",
    "@aws-sdk/util-dynamodb": "^3.370.0",
    "@esm2cjs/p-queue": "^7.3.0",
    "@mux/mux-node": "^7.0.0",
    "@types/set-cookie-parser": "^2.4.3",
    "ably": "^1.2.15",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "jsonwebtoken": "^9.0.1",
    "node-fetch": "^3.2.10",
    "set-cookie-parser": "^2.6.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.68",
    "@types/cors": "^2.8.9",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^22.0.0",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "bestzip": "^2.1.7",
    "del-cli": "^6.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-jest": "^29.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.0.0",
    "ts-auto-guard": "^5.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}
